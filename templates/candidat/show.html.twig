                                                    function imprimer_page(){
                                                        window.print();
                                                    }
                                                </script>
                                                <div class="btn-toolbar" role="toolbar" aria-label="Programmatic control">
                                                    <div class="btn-group btn-group-sm" aria-label="Set Select2 option">
                                                        <form>
                                                            <input id="impression" class="js-programmatic-set-val btn btn-outline-primary mr-1 mb-1s" name="impression" type="button" onclick="imprimer_page()" value="Imprimer cette page" />
                                                        </form>
                                                <table class="table">

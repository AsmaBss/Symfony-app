                                                    </tr>
                                                    <tr>
                                                        <td class="font-weight-bold">Image</td>
                                                        <td>{{ candidat.img }}</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="font-weight-bold">Birthday</td>
                                                        <td>{{ candidat.dateNaissance ? candidat.dateNaissance|date('Y-m-d') : '' }}</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="font-weight-bold">Degree</td>
                                                        <td>{{ candidat.nivEtude }}</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="font-weight-bold">Type </td>
                                                        <td>{{ candidat.typeCandidat }}</td>
                                                    </tr>
                                                    <tr>
                                                        <td class="font-weight-bold">Skills </td>
                                                        {% for skills in candidat.skills %}
                                                        <td>{{ skills.libelle }}</td>
                                                        {% endfor %}
                                                    </tr>

                                                    </tbody>
                                                </table>
                                            </div>
                                            <div class="col-12">
                                                <a href="{{ path('candidat_edit', {'id': candidat.id}) }}" class="btn btn-primary mr-1"><i class="feather icon-edit-1"></i> Edit</a>
                                                <a href="{{ path('candidat_index') }}" class="btn btn-primary mr-1"><i class="feather icon-edit-1"></i> afficher</a>

                                                {{ include('candidat/_delete_form.html.twig') }}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- account end -->

                                                    <img src="/uploads/{{candidat.getImg()}}" alt="avtar img holder"  style="width:50px; height: 50px; object-fit: scale-down; " >
                                                </div>
                                                <div class="user-page-info">
                                                    <p class="mb-0">{{ candidat.getNom ~ " " ~ candidat.getPrenom }}</p>

                                                    <span class="font-small-2">{{ post.date ? post.date|date('Y-m-d') : '' }} AT {{ post.date ? post.date|date('H:i:s') : '' }}</span>
                                                </div>
                                                {% if app.user.id == candidat.id%}
                                                <div class="ml-auto">
                                                    <div style="display: inline-block;">
                                                        <a class="btn btn-flat-primary mr-1 mb-1" href="{{ path('user_post_edit', {'id': post.id}) }}"> <i class="feather icon-edit-1"></i></a>
                                                    </div>
                                                    <div style="display: inline-block;">
                                                        {{ include('user_post/_delete_form.html.twig') }}
                                                    </div>
                                                    <div style="display: inline-block;">
                                                        <a class="btn btn-flat-primary mr-1 mb-1" href="{{ path('user_post_report', {'id': post.id, 'iduser': app.user.id}) }}"><i class="feather icon-flag"></i></a>
                                                    </div>

                                                    <div style="display: inline-block;">
                                                        <a class="btn btn-flat-primary mr-1 mb-1" href="{{ path('user_post_bookmark', {'id': post.id,'iduser': app.user.id}) }}">
                                                            {% if post.bookmarked == 0 %}
                                                                <i class="feather icon-bookmark"></i>
                                                            {% else %}
                                                                <i class="fa fa-bookmark"></i>
                                                            {% endif %}
                                                        </a>
                                                    </div>

                                                </div>
                                                {% endif %}
                                            </div>


                                            <!-------------------------content here---------------------->
                                            <p> {{ post.content |raw }}  </p>


                                            <div class="d-flex justify-content-start align-items-center mb-1">
                                                <div class="d-flex align-items-center">
                                                    <a href="{{ path('user_post_like',{'id': post.id, 'iduser': app.user.id}) }}"><i class="feather icon-heart font-medium-2 mr-50"></i></a>
                                                    <span>{{ post.nblikes }}</span>
                                                </div>

                                                <div class="ml-2">
                                                    <ul class="list-unstyled users-list m-0  d-flex align-items-center">
                                                        <li data-toggle="tooltip" data-popup="tooltip-custom" data-placement="bottom" data-original-title="Trina Lynes" class="avatar pull-up">
                                                            <img class="media-object rounded-circle" src="../../../app-assets/images/portrait/small/avatar-s-1.jpg" alt="Avatar" height="30" width="30">
                                                        </li>
                                                        <li data-toggle="tooltip" data-popup="tooltip-custom" data-placement="bottom" data-original-title="Lilian Nenez" class="avatar pull-up">
                                                            <img class="media-object rounded-circle" src="../../../app-assets/images/portrait/small/avatar-s-2.jpg" alt="Avatar" height="30" width="30">
                                                        </li>
                                                        <li data-toggle="tooltip" data-popup="tooltip-custom" data-placement="bottom" data-original-title="Alberto Glotzbach" class="avatar pull-up">
                                                            <img class="media-object rounded-circle" src="../../../app-assets/images/portrait/small/avatar-s-3.jpg" alt="Avatar" height="30" width="30">
                                                        </li>
                                                        <li data-toggle="tooltip" data-popup="tooltip-custom" data-placement="bottom" data-original-title="George Nordic" class="avatar pull-up">
                                                            <img class="media-object rounded-circle" src="../../../app-assets/images/portrait/small/avatar-s-5.jpg" alt="Avatar" height="30" width="30">
                                                        </li>
                                                        <li data-toggle="tooltip" data-popup="tooltip-custom" data-placement="bottom" data-original-title="Vinnie Mostowy" class="avatar pull-up">
                                                            <img class="media-object rounded-circle" src="../../../app-assets/images/portrait/small/avatar-s-4.jpg" alt="Avatar" height="30" width="30">
                                                        </li>

                                                    </ul>
                                                </div>
                                                <p class="ml-auto d-flex align-items-center">
                                                    <i class="feather icon-message-square font-medium-2 mr-50"></i>77
                                                </p>
                                            </div>
                                            <div class="divider divider-primary">
                                                <div class="divider-text">{{ post.comments|length }} Comments</div>
                                            </div>
                                            {% for comment in post.comments %}
                                                <div class="d-flex justify-content-start align-items-center mb-2">

                                                    <div class="avatar mr-50">
                                                        <img src="/uploads/{{comment.user.getImg()}}" alt="Avatar" height="30" width="30" style="object-fit: scale-down;">
                                                    </div>
                                                    <div class="user-page-info">
                                                        <h6 class="mb-0">{{ comment.user.nom ~ " " ~ comment.user.prenom }}</h6>

                                                        <span class="font-small-2">{{ comment.content }}</span>

                                                    </div>
                                                    <br>


                                                    <div class="ml-auto cursor-pointer">
                                                {% if app.user.id == candidat.id %}
                                                        <div style="display: inline-block;">
                                                        {{ include('user_post/_deleteComment_form.html.twig') }}
                                                        </div>
                                                        <div style="display: inline-block;">

                                                        <a class="btn btn-flat-primary mr-1 mb-1" href="{{ path('commentFront_edit', {'id': comment.id,'iduser':app.user.id}) }}"> <i class="feather icon-edit-1"></i></a>
                                                        </div>

                                                    </div>
                                                    {% endif %}
                                                </div>

                                            {% endfor %}


                                            {% if is_granted('ROLE_CANDIDATE') %}
                                            <a class="btn btn-sm btn-primary" href="{{ path('user_post_newcomment', {'id': post.id, 'iduser': app.user.id}) }}">Post Comment</a>
                                            {% endif %}


                                        </div>
                                    </div>

                                {% endfor %}
                            </div>






                            <!-----------------------------------END POST--------------------------------------------------------------->

                            <div class="col-lg-3 col-12">
                                <div class="card">
                                    <div class="card-header">
                                        <h4>Latest Photos</h4>
                                    </div>
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-md-4 col-6 user-latest-img">
                                                <img src="../../../app-assets/images/profile/user-uploads/user-01.jpg" class="img-fluid mb-1 rounded-sm" alt="avtar img holder">
                                            </div>
                                            <div class="col-md-4 col-6 user-latest-img">

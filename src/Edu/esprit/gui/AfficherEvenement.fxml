<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@dark-theme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Edu.esprit.gui.AfficherEvenementController">
   <children>
      <TableView fx:id="afficher_evenement" layoutX="27.0" layoutY="18.0" prefHeight="330.0" prefWidth="517.0">
        <columns>
          <TableColumn fx:id="clm_date" prefWidth="75.0" text="Date" />
          <TableColumn fx:id="clm_adr" prefWidth="75.0" text="Adresse" />
            <TableColumn fx:id="clm_titre" prefWidth="75.0" text="Titre" />
            <TableColumn fx:id="clm_desc" prefWidth="75.0" text="Description" />
            <TableColumn fx:id="clm_part" prefWidth="75.0" text="Nbr_participants" />
            <TableColumn fx:id="clm_max" prefWidth="76.0" text="Nbr_max" />
            <TableColumn fx:id="clm_img" prefWidth="64.0" text="img" />
        </columns>
      </TableView>
      <Button fx:id="btn_del" layoutX="434.0" layoutY="361.0" mnemonicParsing="false" onAction="#deleteEvent" text="Delete" />
      <Button fx:id="btn_ajout" layoutX="557.0" layoutY="30.0" mnemonicParsing="false" onAction="#add_event" text="Add" />
      <Button fx:id="eventEdit" layoutX="558.0" layoutY="65.0" mnemonicParsing="false" onMouseClicked="#modiferClicked" text="Edit" />
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@dark-theme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Edu.esprit.gui.AfficherArticleController">
   <children>
      <TableView fx:id="afficher_article" layoutX="53.0" layoutY="48.0" prefHeight="330.0" prefWidth="455.0" stylesheets="@../../../../../../../Bureau/dark-theme.css">
         <columns>
            <TableColumn fx:id="clm_titre" prefWidth="134.0" text="Titre" />
            <TableColumn fx:id="clm_desc" prefWidth="97.0" text="Description" />
            <TableColumn fx:id="clm_date" prefWidth="117.0" text="Date" />
            <TableColumn fx:id="clm_img" prefWidth="105.0" text="img" />
         </columns>
      </TableView>
      <Button fx:id="btn_del" layoutX="524.0" layoutY="309.0" mnemonicParsing="false" onAction="#deleteArticle" text="Delete" />
      <Button fx:id="btn_ajout" layoutX="531.0" layoutY="77.0" mnemonicParsing="false" onAction="#add_article" text="Add" />
      <Button fx:id="eventEdit" layoutX="531.0" layoutY="127.0" mnemonicParsing="false" onMouseClicked="#modiferClicked" text="Edit" />
      <TextField fx:id="search" layoutX="80.0" layoutY="14.0" onInputMethodTextChanged="#searchTextChanged" onKeyReleased="#searchKeyRelaesed" />
   </children>
</AnchorPane>
